{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": [false],
    "max-line-length": [true, { "limit": 120}],
    "no-increment-decrement": [false],
    "no-else-after-return": [false],
    "align": [false],
    "no-this-assignment": [false],
    "variable-name": [false],
    "function-name": [false],
    "brace-style": ["error", "stroustrup"],
    "prefer-array-literal": false,
    "class-name": false
  },
  "globals": {
    "USER": "writable"
  }
}